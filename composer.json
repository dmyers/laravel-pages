{
    "name": "dmyers/laravel-pages",
    "description": "A simple static pages package for Laravel.",
    "keywords": ["laravel", "pages", "static pages"],
    "license": "MIT",
    "authors": [
        {
            "name": "Derek Myers",
            "email": "arcticpro@gmail.com"
        }
    ],
    "require": {
        "php": "^5.4 || ^7.0 || ^8.0",
        "illuminate/support": "^5.0|^6.0|^7.0",
    },
    "suggest": {
        "anahkiasen/flatten": "Add full page caching for speed."
    },
    "autoload": {
        "classmap": [
            "src/controllers"
        ],
        "psr-0": {
            "Dmyers\\Pages": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dmyers\\Pages\\PagesServiceProvider"
            ],
            "aliases": {
                "Pages": "Dmyers\\Pages\\Facade"
            }
        }
    },
    "minimum-stability": "dev"
}
