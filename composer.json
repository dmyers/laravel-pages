{
    "name": "dmyers/laravel-pages",
    "description": "A simple static pages package for Laravel 5.",
    "keywords": ["laravel", "pages", "static pages"],
    "license": "MIT",
    "authors": [
        {
            "name": "Derek Myers",
            "email": "arcticpro@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "~5.0"
    },
    "suggest": {
        "anahkiasen/flatten": "Add full page caching for speed."
    },
    "autoload": {
        "classmap": [
            "src/controllers"
        ],
        "psr-0": {
            "Dmyers\\Pages": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dmyers\\Pages\\PagesServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
